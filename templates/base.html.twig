<body>
<nav class="navbar navbar-light bg-light">
    <a class="navbar-brand" href="{{ path('home') }}">Accueil</a>
    {% if app.user %}
        <a class="btn btn-outline-info" href="{{ path('membre') }}">Page membre</a>
        {% if is_granted('ROLE_ADMIN') %}
            <a class="btn btn-outline-warning" href="{{ path('admin') }}">Page admin</a>
        {% endif %}
        <div>
            Bonjour {{ app.user.email }} <a class="btn btn-sm btn-danger" href="{{ path('app_logout') }}">DÃ©connexion</a>
        </div>
    {% else %}
        <div>
            <a class="btn btn-sm btn-primary" href="{{ path('app_utilisateur_new') }}">S'inscrire</a>
            <a class="btn btn-sm btn-success" href="{{ path('app_login') }}">Se connecter</a>
        </div>
    {% endif %}
</nav>
<!-- END nav -->

<div class="container">
    {% if message is defined %}
        <div class="alert alert-danger">
            {{ message }}
        </div>
    {% endif %}
    {% block body %}
    {% endblock %}
</div>



